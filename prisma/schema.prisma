generator client {
  provider = "prisma-client-js"
  output  = "../src/generated/prisma"
}

datasource db {
  provider = "mysql"
 
}

model ApiKey {
  id        Int      @id @default(autoincrement())
  hashedKey String   @unique
  roles     Json
  owner     String?
  status    String   @default("active")
  createdAt DateTime @default(now())
}
